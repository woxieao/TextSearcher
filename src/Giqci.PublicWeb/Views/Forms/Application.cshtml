                    } else {
                        $scope.dialogProductList = null;
                        //alertService.add('danger', "正确的备案号格式为[ICIP+14个数字]或[10个数字]", 3000);
                        layer.alert("正确的备案号格式为[ICIP+14个数字]或[10个数字]");
