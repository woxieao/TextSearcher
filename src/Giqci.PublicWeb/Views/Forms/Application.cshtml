                                if (data.result) {
                                    $scope.getGoodsList();
                                    layer.alert("提交成功", function (l) { $scope.showProductList(); layer.close(l) });
                                }
                                var _tipType = data.result ? "success" : "danger";
                                //alertService.add(_tipType, data.msg || "未知错误", 3000);
                                layer.alert(response.msg || "未知错误");
                            }, $scope);
                        }
                        layer.close(l);
                    }); 
                };
